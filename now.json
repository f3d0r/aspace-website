{
    "alias": ["trya.space"],
    "builds": [{
            "src": "*.html",
            "use": "@now/html-minifier"
        },
        {
            "src": "privacy/*.html",
            "use": "@now/html-minifier"
        },
        {
            "src": "js/*.js",
            "use": "@now/static"
        },
        {
            "src": "css/*.css",
            "use": "@now/static"
        },
        {
            "src": "img/emoji/*.png",
            "use": "@now/static"
        },
        {
            "src": "img/abstract_icons/*.svg",
            "use": "@now/static"
        },
        {
            "src": "img/social/black/*.svg",
            "use": "@now/static"
        },
        {
            "src": "img/social/white/*.svg",
            "use": "@now/static"
        },
        {
            "src": "img/icons/*.svg",
            "use": "@now/static"
        },
        {
            "src": "*.png",
            "use": "@now/static"
        },
        {
            "src": "robots.txt",
            "use": "@now/static"
        },
        {
            "src": "sitemap.xml",
            "use": "@now/static"
        }
    ],
    "github": {
        "autoAlias": true,
        "autoJobCancelation": true,
        "enabled": true,
        "silent": true
    },
    "name": "aspace-website",
    "regions": ["all"],
    "version": 2
}