{
    "alias": ["aspace.fedor.me", "www.aspace.fedor.me"],
    "builds": [{
            "src": "*.html",
            "use": "@now/static"
        },
        {
            "src": "privacy/*.html",
            "use": "@now/static"
        },
        {
            "src": "js/*.js",
            "use": "@now/static"
        },
        {
            "src": "css/*.css",
            "use": "@now/static"
        },
        {
            "src": "img/emoji/*.png",
            "use": "@now/static"
        },
        {
            "src": "img/icons/abstract_icons/*.svg",
