        {
            "src": "img/other/*.jpeg",
            "use": "@now/static"
        },
        {
            "src": "img/plugins/*.svg",
            "use": "@now/static"
        },
        {
            "src": "img/other/*.png",
            "use": "@now/static"
        },
        {
            "src": "img/screenshots/*.png",
            "use": "@now/static"
        },
        {
            "src": "*.png",
            "use": "@now/static"
        },
        {
            "src": "robots.txt",
            "use": "@now/static"
        },
        {
            "src": "sitemap.xml",
            "use": "@now/static"
        }
    ],
    "github": {
        "autoAlias": true,
        "autoJobCancelation": true,
        "enabled": true,
        "silent": true
    },
    "name": "aspace-website",
    "regions": ["all"],
    "version": 2
}